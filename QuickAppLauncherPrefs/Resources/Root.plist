<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>

    <!-- Slider for Button Scale -->
    <dict>
        <key>cell</key>
        <string>PSSliderCell</string>
        <key>default</key>
        <real>1.0</real>
        <key>defaults</key>
        <string>com.yourname.quickapplauncher</string>
        <key>key</key>
        <string>QALButtonScale</string>
        <key>label</key>
        <string>Button Scale</string>
        <key>min</key>
        <real>0.5</real>
        <key>max</key>
        <real>2.0</real>
        <key>showValue</key>
        <true/>
    </dict>

    <!-- Text field for Bundle ID -->
    <dict>
        <key>cell</key>
        <string>PSEditTextCell</string>
        <key>default</key>
        <string>com.apple.Preferences</string>
        <key>defaults</key>
        <string>com.yourname.quickapplauncher</string>
        <key>key</key>
        <string>QALBundleID</string>
        <key>label</key>
        <string>App Bundle ID</string>
        <key>placeholder</key>
        <string>e.g. com.apple.mobilesafari</string>
        <key>keyboard</key>
        <string>alphabet</string>
        <key>noAutoCorrect</key>
        <true/>
    </dict>

</array>
</plist>

